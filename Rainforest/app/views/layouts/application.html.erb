<!DOCTYPE html>
<html>
  <head>
    <title>Rainforest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>



  <body class="container">
    <header class="header">
      <h1 class="logo">Rainforest</h1>
      <nav class="navbar">
        <a><%= link_to 'Home', root_path %></a>
        <a><%= link_to 'Products', products_path %></a>
        <a><%= link_to 'New Products', new_product_path %></a>
      </nav>
    </header>

    <section class="wrapper">
      <%= yield %>
    </section>

    <footer>
      <h5>Hasan | Karen | Gustavo  </h5>
    </footer>
  </body>
</html>
